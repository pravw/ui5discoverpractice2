<mvc:View controllerName="com.po.countdowntimer.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns="sap.m">
    <Page id="page" class="sapUiResponsiveContentPadding" title="{i18n>title}">
    <VBox id="vbox">
     <Input id="input"  class="sapUiTinyMarginEnd"      width="20rem" maxLength="10"/>
     <ComboBox id="idCombobox" placeholder="category">
     <items>
     <core:Item  id="comboBoxItem1" key="0" text="food"/>
      <core:Item  id="comboBoxItem2" key="1" text="food"/>
       <core:Item  id="comboBoxItem3" key="2" text="Beverages"/>
        <core:Item  id="comboBoxItem4" key="3" text="Electronics"/>
   
     </items>
     </ComboBox>
    </VBox>
   
    <Button id="button" text="{i18n>selectMe}" press="onpress"/>
    <List id="idList"  class="sapUiTinyMarginTop"  backgroundDesign= "Transparent" headerText="Products" mode="Delete" delete="onPressDelete">
    <ObjectListItem id="idObject"   title="Macc bock"  number="1000" numberUnit="Eur">
    <attributes>
    <ObjectAttribute id="objectAtribute1"   title="Categories" text="Electronics"/>
    <ObjectAttribute id="objectAtribute2"   title="ReleaseDate" text="22-12-2025"/>
    </attributes>
    <firstStatus>
    <ObjectStatus id="objectStatus"  text="available" state="Success" />
    </firstStatus>
    </ObjectListItem>
    </List>
    <footer>
    <Toolbar id="tl">
    <Text id= "tx" text="I AM FOOTER"/>
    </Toolbar>
    </footer>
    </Page>
</mvc:View>